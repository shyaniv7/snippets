{
	// Place your snippets for typescriptreact here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	"Print to console": {
		"prefix": "log",
		"body": [
			"console.log('$1');",
			"$2"
		],
		"description": "Log output to console"
	},
	"React FC": {
		"prefix": "rfc",
		"body": "import React from 'react';\n\nconst ${1:MyComponent}: React.FC = () => {\n\treturn (<div>${1:MyComponent}</div>);\n};\n\nexport default ${1:MyComponent};\n",
		"description": "React functional component"
	},
	"Define Type": {
		"prefix": "deftype",
		"body": "type ${1:TypeName} = {\n\t${2:FieldName}: ${3:FieldType};\n};\n",
		"description": "Type Script Type Definition"
	},
	"Define Enum": {
		"prefix": "defenum",
		"body": "enum ${1:EnumName} {\n\t${2:Key} = ${3:Value},\n};\n",
		"description": "Enum Definition"
	},
	"useState": {
		"prefix": "defusestate",
		"body": "const [${1:value}, ${2:setValue}] = React.useState(${3:initialValue});\n",
		"description": "React useState hook"
	},
	"useEffect": {
		"prefix": "defuseeffect",
		"body": "React.useEffect(() => {\n\t${1:doSomething}\n});\n",
		"description": "React useEffect hook"
	}
}
